SELECT column1 AS c1
from table1 AS t1, table2 AS t2 --Sets columns name to c locally.

Select Concat (column1, " ", column2) AS c3 -- combines column1 and column2 and outputs as c3

Select x.column1, x.column2, y.column1, y.column2
from table1 AS x, table2 AS y
where x.column1 < 12 AND y.column2 < 5;